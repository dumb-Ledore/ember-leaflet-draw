{
  "name": "ember-leaflet-draw-controls",
  "version": "0.7.3",
  "description": "Provides feature drawing functionality for Ember-Leaflet, based on wrapping the Leaflet Draw library",
  "author": "Steven Heinrich",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
