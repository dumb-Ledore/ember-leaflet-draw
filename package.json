  "license": "MIT",
  "dependencies": {
    "@ember/jquery": "^0.5.2",
    "broccoli-funnel": "^3.0.3",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-composability-tools": "^0.0.12",
    "ember-invoke-action": "^1.4.0",
    "font-awesome": "~4.7.0",
    "leaflet": "^1.6.0",
    "leaflet-draw": "^1.0.4",
    "resolve": "^1.3.2",
    "semantic-ui": "^2.4.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.12.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-less": "1.5.3",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-code-snippet": "1.9.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-font-awesome": "3.1.1",
    "ember-leaflet": "^3.0.9",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "5.0.1",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^9.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.4",
    "semantic-ui-ember": "^3.0.4"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "resolutions": {
    "graceful-fs": "4.2.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-leaflet"
    ],
    "demoURL": "https://stevenheinrich.github.io/ember-leaflet-draw/"
